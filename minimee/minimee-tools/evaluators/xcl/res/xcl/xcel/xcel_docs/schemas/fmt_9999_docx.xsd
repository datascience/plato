		<xs:simpleType name="fmt_9999_docx" id="properties">
			<xs:restriction base="xs:string">
				<xs:enumeration value='fontChange' id='idNULL2'>
					<xs:annotation>
						<xs:documentation>
							[Compatibility: PDF 1.4]
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='filelength' id='id5'>
					<xs:annotation>
						<xs:documentation>
							The length in bytes of the complete file
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='byteOrder' id='id6'>
					<xs:annotation>
						<xs:documentation>
							Ordering of bytes for multi-byte data values within a file
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='compression' id='id18'>
					<xs:annotation>
