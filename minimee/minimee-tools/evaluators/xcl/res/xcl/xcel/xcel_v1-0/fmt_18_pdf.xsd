<!-- extensible characterisation language: properties' name definitions library for XCDL and XCEL documents -->
<!-- created by PLANETS, PC2, University of Cologne (HKI )-->

<xs:schema 
    xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    xmlns="http://www.planets-project.eu/xcl/schemas/xcl"
    xmlns:tns="http://www.planets-project.eu/xcl/schemas/xcl"
    targetNamespace="http://www.planets-project.eu/xcl/schemas/xcl"
    elementFormDefault="qualified">
    
		<xs:simpleType name="fmt18_pdf" id="properties">
			<xs:restriction base="xs:string">
				<xs:enumeration value='fontChange' id='idNULL2'>
					<xs:annotation>
						<xs:documentation>
							[Compatibility: PDF 1.4]
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='creationSoftware' id='id3'>
					<xs:annotation>
						<xs:documentation>
							Word 2002 and Word 2003 allow the RTF emitter application to stamp the document with its name,
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='signature' id='id4'>
					<xs:annotation>
						<xs:documentation>
							A sequence of bytes that uniquely identifies a certain fileformat
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='byteOrder' id='id6'>
					<xs:annotation>
						<xs:documentation>
							Ordering of bytes for multi-byte data values within a file (intel or motorola)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='normData' id='id7'>
					<xs:annotation>
						<xs:documentation>
							XCL specific data, sp. reference for description
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='compressionAlgorithm' id='id18'>
					<xs:annotation>
						<xs:documentation>
							algorithm applied to file data for the purpose of minimizing storage size
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='imageData' id='id19'>
					<xs:annotation>
						<xs:documentation>
							data block to be interpreted as image data
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='artist' id='id31'>
					<xs:annotation>
						<xs:documentation>
							Person who created the document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='copyrightNote' id='id35'>
					<xs:annotation>
						<xs:documentation>
							Copyright notice of the person or organization that claims the copyright for the file
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='documentCreationDate' id='id36'>
					<xs:annotation>
						<xs:documentation>
							Date and time of file creation [cit. TIFF 6.0]  [Compatibility: TIFF 6.0]
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='pageBreak' id='id182'>
					<xs:annotation>
						<xs:documentation>
							Contentproperty describing when a textual content breaks to a new page (Unit not yet decided)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='textualAnnotation' id='id58'>
					<xs:annotation>
						<xs:documentation>
							keyword for textual information associated with the object
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='openTypeFont' id='id221'>
					<xs:annotation>
						<xs:documentation>
							filetype for fonts
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='trueType3' id='id220'>
					<xs:annotation>
						<xs:documentation>
							filetype for fonts
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='trueType1' id='id219'>
					<xs:annotation>
						<xs:documentation>
							filetype for fonts
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='pageLayout' id='id77'>
					<xs:annotation>
						<xs:documentation>
							page layout to be used when the document is opened (should be something like "UsLetter or DinA4").
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='documentTitle' id='id78'>
					<xs:annotation>
						<xs:documentation>
							title of the document, if different of fileName (possibly only accuring in pdf-files)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='documentModificationDate' id='id82'>
					<xs:annotation>
						<xs:documentation>
							date and time of last object modification
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='numberOfPages' id='id84'>
					<xs:annotation>
						<xs:documentation>
							number of pages in a rendered textdocument
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='defaultFont' id='id268'>
					<xs:annotation>
						<xs:documentation>
							defaultFont (if font is not found on the system)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='dinA6' id='id267'>
					<xs:annotation>
						<xs:documentation>
							105x148 cm (width x hight) size of textdocument
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='fontFiletype' id='id211'>
					<xs:annotation>
						<xs:documentation>
							containes which filetype the font is: i.e.: truetype (type 1 or 3), postscript, openType
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='fontEncodingType' id='id97'>
					<xs:annotation>
						<xs:documentation>
							can be StandardEncoding, MacRomanEncoding, WinAnsiEncoding, and PDF-  DocEncoding.
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='uppercaseLetters' id='id212'>
					<xs:annotation>
						<xs:documentation>
							defines if all letters are in upper case in a word (called AllCap in pdf)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='dinA5' id='id266'>
					<xs:annotation>
						<xs:documentation>
							148x210 cm (width x hight) size of textdocument
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='dinA4' id='id265'>
					<xs:annotation>
						<xs:documentation>
							210x297 cm (width x hight) size of textdocument
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='customPageSize' id='id264'>
					<xs:annotation>
						<xs:documentation>
							PageSize of Textdocument if different from standardSizes. Messured in Unit of TextDocument
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='marginRight' id='id263'>
					<xs:annotation>
						<xs:documentation>
							distance between text and document-edge in unit of document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='marginLeft' id='id262'>
					<xs:annotation>
						<xs:documentation>
							distance between text and document-edge in unit of document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='fileSize' id='id120'>
					<xs:annotation>
						<xs:documentation>
							The length in bytes of the complete file
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='headerSize' id='id122'>
					<xs:annotation>
						<xs:documentation>
							Size of the fileheader in bytes
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='font' id='id159'>
					<xs:annotation>
						<xs:documentation>
							font name in text-based formats (used in OOXML)  and PDF!
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='marginTop' id='id261'>
					<xs:annotation>
						<xs:documentation>
							distance between text and document-edge in unit of document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='littleEndian' id='id225'>
					<xs:annotation>
						<xs:documentation>
							referring to the byteOrder of an operationSysten
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='hostMachineOS' id='id140'>
					<xs:annotation>
						<xs:documentation>
							The computer and/or operating system in use at the time of object creation 
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='marginBottom' id='id260'>
					<xs:annotation>
						<xs:documentation>
							distance between text and document-edge in unit of document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='embeddedObject' id='id146'>
					<xs:annotation>
						<xs:documentation>
							A general indication of the kind of data contained in a subfile or else embedded file. should be further separated into textData, imageData, audioData...
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='bigEndian' id='id224'>
					<xs:annotation>
						<xs:documentation>
							Referring to the byteOrder of an operationsystem
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='textData' id='id214'>
					<xs:annotation>
						<xs:documentation>
							data block to be interpreted as TextData, maybe structured inside a paragraph in a textfile but also in a png-file...
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='fontSize' id='id158'>
					<xs:annotation>
						<xs:documentation>
							font Size in text-based formats messured in point (in ooxml: half-points (1/144 of an inch))
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='fontColour' id='id160'>
					<xs:annotation>
						<xs:documentation>
							font color in text formats (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='bold' id='id161'>
					<xs:annotation>
						<xs:documentation>
							text-property bold (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='italic' id='id162'>
					<xs:annotation>
						<xs:documentation>
							text-property italic (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='underline' id='id163'>
					<xs:annotation>
						<xs:documentation>
							text-property underline (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='strike' id='id164'>
					<xs:annotation>
						<xs:documentation>
							text-property strike (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='doubleStrike' id='id165'>
					<xs:annotation>
						<xs:documentation>
							text-property doubleStrike (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='shadow' id='id166'>
					<xs:annotation>
						<xs:documentation>
							text-property shadow (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='smallCaps' id='id167'>
					<xs:annotation>
						<xs:documentation>
							text-property smallCaps (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='caps' id='id168'>
					<xs:annotation>
						<xs:documentation>
							text-property caps (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='hidden' id='id169'>
					<xs:annotation>
						<xs:documentation>
							text is hidden (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='footnote' id='id170'>
					<xs:annotation>
						<xs:documentation>
							footnote (used in OOXML)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='postscriptFont' id='id222'>
					<xs:annotation>
						<xs:documentation>
							filetype for font
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='lineBreak' id='id183'>
					<xs:annotation>
						<xs:documentation>
							Content-property describing when a string content breaks to a new line (Unit not yet decided)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='hypertextLink' id='id184'>
					<xs:annotation>
						<xs:documentation>
							clickable link to URL, which should open a browser-window, possible in msdocs, ooxml and pdf
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='comment' id='id185'>
					<xs:annotation>
						<xs:documentation>
							Comment inserted by user in certain programm
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='formField' id='id186'>
					<xs:annotation>
						<xs:documentation>
							form field which allows a user to enter strings in a pdf-viewer programm
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='passwordProtectedAction' id='id187'>
					<xs:annotation>
						<xs:documentation>
							describes if and which certain operations are allowed to all users (like printing, editing...)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='lastModifiedBy' id='id188'>
					<xs:annotation>
						<xs:documentation>
							Names the last editor (person). Used in OOXML
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='numberOfCharacters' id='id189'>
					<xs:annotation>
						<xs:documentation>
							Number of characters in document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='documentLanguage' id='id190'>
					<xs:annotation>
						<xs:documentation>
							The language of the content in a document. Can be spoken or written (i. e. in audio-, video-, text-objects)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='textAdjustment' id='id191'>
					<xs:annotation>
						<xs:documentation>
							Adjustment of text in document (left, center, right, justification)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='lineSpacing' id='id192'>
					<xs:annotation>
						<xs:documentation>
							Space between lines in textdocument. Unit in point or according to Unit of document?
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='pageNumber' id='id195'>
					<xs:annotation>
						<xs:documentation>
							The number of the page on which the content is from (either scanned pages for image-objects but also text documents)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='tableNumberOfRows' id='id201'>
					<xs:annotation>
						<xs:documentation>
							Number of Rows in a Table
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='tableNumberOfColumns' id='id202'>
					<xs:annotation>
						<xs:documentation>
							Number of Columns in a Table
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='fileName' id='id205'>
					<xs:annotation>
						<xs:documentation>
							Name of the file
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='pageBackroundColour' id='id206'>
					<xs:annotation>
						<xs:documentation>
							describes the backround colour of the document-page (messured according to colourSpace of Document)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='paragraph' id='id207'>
					<xs:annotation>
						<xs:documentation>
							describes structured continuous text data
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='centerAdjustment' id='id269'>
					<xs:annotation>
						<xs:documentation>
							adjustment of Text
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='leftAdjustment' id='id270'>
					<xs:annotation>
						<xs:documentation>
							default TextAdjustment
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='rightAdjustment' id='id271'>
					<xs:annotation>
						<xs:documentation>
							Adjustment of Text
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='justifyAdjustment' id='id272'>
					<xs:annotation>
						<xs:documentation>
							justify Text Adjustment
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='cellHeight' id='id273'>
					<xs:annotation>
						<xs:documentation>
							cellHeight of cell in Table, messured in Unit of Document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='cellWidth' id='id274'>
					<xs:annotation>
						<xs:documentation>
							cellWidth of cell in Table, messured in Unit of Document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='cell' id='id275'>
					<xs:annotation>
						<xs:documentation>
							defines a cell in a table
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='columnNumber' id='id276'>
					<xs:annotation>
						<xs:documentation>
							nth column in a table
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='rowNumber' id='id277'>
					<xs:annotation>
						<xs:documentation>
							nth row in a table
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='tableBorder' id='id278'>
					<xs:annotation>
						<xs:documentation>
							width of outline in a table (messured in documentUnit)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='tableBorderColour' id='id279'>
					<xs:annotation>
						<xs:documentation>
							Colour of tableBorder in ColourSpace of Document
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='watermark' id='id285'>
					<xs:annotation>
						<xs:documentation>
							type of watermark to be used in file (digimark, stirmark etc...)
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='macOsEncoding' id='id286'>
					<xs:annotation>
						<xs:documentation>
							encodingType for MacOs Operationsystem
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_9' id='id287'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi Encoding for turkish
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_15' id='id288'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for Latin 9
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_3' id='id289'>
					<xs:annotation>
						<xs:documentation>
							ISO-Ansi-Encoding for Latin 3
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_4' id='id290'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for baltic
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_2' id='id291'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for centralEuropean
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_6' id='id292'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for arabic
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_5' id='id293'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for cyrillic
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_7' id='id294'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for greek
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_8' id='id295'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for hebrew
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='iso8859_1' id='id296'>
					<xs:annotation>
						<xs:documentation>
							Iso-Ansi-Encoding for westeuropean
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='utf7' id='id297'>
					<xs:annotation>
						<xs:documentation>
							CharacterEncodingType
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='utf8' id='id298'>
					<xs:annotation>
						<xs:documentation>
							CharacterEncodingType
						</xs:documentation>
					</xs:annotation>
				</xs:enumeration>
				<xs:enumeration value='utf16' id='id299'>
					<xs:annotation>
						<xs:documentation>
							CharacterEncodingType
						</xs:documentation>
