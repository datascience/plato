<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns="http://taverna.sf.net/2008/xml/t2flow" version="1" producedBy="taverna-2.4.0">
  <dataflow id="{{id}}" role="top">
    <name>{{name}}</name>
    <inputPorts>
      {{#inputPorts}}
      <port>
        <name>{{name}}</name>
        <depth>{{depth}}</depth>
        <granularDepth>0</granularDepth>
        <annotations>
          {{> data/t2flow/semanticAnnotation}}
        </annotations>
      </port>
      {{/inputPorts}}
    </inputPorts>
    <outputPorts>
      {{#outputPorts}}
      <port>
        <name>{{name}}</name>
        <annotations>
          {{> data/t2flow/semanticAnnotation}}
        </annotations>
      </port>
      {{/outputPorts}}
    </outputPorts>
    <processors>{{#processors}}
      {{#template}}{{/template}}
      {{/processors}}</processors>
    <conditions />
    <datalinks>
      {{#datalinks}}
      <datalink>
        <sink type="{{sinkType}}">
          {{#sinkProcessor}}
          <processor>{{sinkProcessor}}</processor>
          {{/sinkProcessor}}
          <port>{{sinkPort}}</port>
        </sink>
        <source type="{{sourceType}}">
          {{#sourceProcessor}}
          <processor>{{sourceProcessor}}</processor>
          {{/sourceProcessor}}
          <port>{{sourcePort}}</port>
        </source>
      </datalink>
      {{/datalinks}}
    </datalinks>
    <annotations>
      <annotation_chain_2_2 encoding="xstream">
        <net.sf.taverna.t2.annotation.AnnotationChainImpl xmlns="">
          <annotationAssertions>
            <net.sf.taverna.t2.annotation.AnnotationAssertionImpl>
              <annotationBean class="net.sf.taverna.t2.annotation.annotationbeans.IdentificationAssertion">
                <identification>{{id}}</identification>
              </annotationBean>
              <date>{{date}}</date>
              <creators />
              <curationEventList />
            </net.sf.taverna.t2.annotation.AnnotationAssertionImpl>
          </annotationAssertions>
        </net.sf.taverna.t2.annotation.AnnotationChainImpl>
      </annotation_chain_2_2>
      {{#author}}
      <annotation_chain encoding="xstream">
        <net.sf.taverna.t2.annotation.AnnotationChainImpl>
          <annotationAssertions>
            <net.sf.taverna.t2.annotation.AnnotationAssertionImpl>
              <annotationBean class="net.sf.taverna.t2.annotation.annotationbeans.Author">
                <text>{{author}}</text>
              </annotationBean>
              <date>{{date}}</date>
              <creators />
              <curationEventList />
            </net.sf.taverna.t2.annotation.AnnotationAssertionImpl>
          </annotationAssertions>
        </net.sf.taverna.t2.annotation.AnnotationChainImpl>
      </annotation_chain>
      {{/author}}
      <annotation_chain encoding="xstream">
        <net.sf.taverna.t2.annotation.AnnotationChainImpl>
          <annotationAssertions>
            <net.sf.taverna.t2.annotation.AnnotationAssertionImpl>
              <annotationBean class="net.sf.taverna.t2.annotation.annotationbeans.DescriptiveTitle">
                <text>{{name}}</text>
              </annotationBean>
              <date>{{date}}</date>
              <creators />
              <curationEventList />
            </net.sf.taverna.t2.annotation.AnnotationAssertionImpl>
          </annotationAssertions>
        </net.sf.taverna.t2.annotation.AnnotationChainImpl>
      </annotation_chain>
      {{> data/t2flow/semanticAnnotation}}
    </annotations>
  </dataflow>
  {{#dataflows}}
  {{{content}}}
  {{/dataflows}}
</workflow>